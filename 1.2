#include <iostream>
#include <string>
using namespace std;

int main() {
    string s;
    cout << "Enter a line: ";
    getline(cin, s);

    if (s.size() >= 2 && s[0] == '/' && s[1] == '/') {
        cout << "Single-line comment (//)\n";
    }
    else if (s.size() >= 4 && s[0] == '/' && s[1] == '*' &&
             s[s.size()-2] == '*' && s[s.size()-1] == '/') {
        cout << "Multi-line comment (/* */)\n";
    }
    else {
        cout << "Not a comment\n";
    }
    return 0;
}
