#include <iostream>
#include <fstream>
#include <string>
#include <set>
#include <cctype>
using namespace std;

bool isKeyword(string s) {
    string kw[] = {"int","float","char","double","if","else","for",
                   "while","do","switch","case","return","static"};
    for (string k : kw)
        if (s == k) return true;
    return false;
}

bool isIdentifier(string s) {
    if (!(isalpha(s[0]) || s[0] == '_')) return false;
    for (char c : s)
        if (!(isalnum(c) || c == '_')) return false;
    return true;
}

int main() {
    ifstream fin("input.txt");
    string word;
    set<string> keywords, identifiers;

    while (fin >> word) {
        if (isKeyword(word))
            keywords.insert(word);
        else if (isIdentifier(word))
            identifiers.insert(word);
    }

    cout << "Keywords:\n";
    for (auto k : keywords) cout << k << " ";

    cout << "\nIdentifiers:\n";
    for (auto i : identifiers) cout << i << " ";

    fin.close();
    return 0;
}
